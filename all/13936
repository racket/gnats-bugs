 >>     >>
 >>     >>                     ;; run-test : test -> (list-of test-result)
 >>     >>                     ;;
 >>     >>                     ;; Run test returning a tree of test-results.
 >>     >>                  Results are
 >>     >>                     ;; ordered L-to-R as they occur in the tree.
 >>     >>                     (define (run-test test)
 >>     >>                       (reverse
 >>     >>                        (fold-test-results
 >>     >>                         (lambda (result seed) (cons result seed))
 >>     >>                         (list)
 >>     >>                         test)))
