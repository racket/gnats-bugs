>How-To-Repeat:
#lang racket
(require racket/control)

(struct deferred (promise))

(define-syntax defer
  (syntax-rules ()
    ((defer ?x) (deferred (delay ?x)))))

(define (undefer d)
  (force (deferred-promise d)))

(define (explore maxdepth choices)
  (define (loop this-prob depth down? choices ans susp)
    (if (null? choices)
        (values ans susp)
        (let ((prob (car (car choices)))
              (branch (cdr (car choices)))
              (rest (cdr choices)))
          (if (deferred? branch)
              (if down?
                  (let-values (((ans susp)
                                (loop (* prob this-prob)
                                      (+ 1 depth)
                                      (or (not maxdepth) (< depth maxdepth))
                                      (undefer branch)
                                      ans susp)))
                    (loop this-prob depth down? rest ans susp))
                  (loop this-prob depth down? rest ans (cons (cons (* prob this-prob) branch) susp)))
              (loop this-prob depth down? rest
                    (dict-update ans branch (lambda (old) (+ old (* prob this-prob))) 0)
                    susp)))))

  (let-values (((ans susp) (loop 1.0 0 #t choices '() '())))
    (foldl (lambda (p a)
             (cons (cons (cdr p) (car p)) a))
           susp
           ans)))

(define (normalize choices)
  (let ((total (foldl (lambda (p acc) (+ (car p) acc)) 0 choices)))
    (map (lambda (p)
           (cons (/ (car p) total)
                 (cdr p)))
         choices)))

(define (pv-unit x)
  (list (cons 1.0 x)))
(define (pv-bind m f)
  (map (lambda (p)
         (let ((prob (car p))
               (branch (cdr p)))
           (if (deferred? branch)
               (cons prob (defer (pv-bind (undefer branch) f)))
               (cons prob (defer (f branch))))))
       m))

#|
(define b pv-unit)

(define (dist ch)
  (map (lambda (p)
         (cons (car p) (cdr p)))
       ch))

(define (neg e)
  (pv-bind e
           (lambda (x)
             (pv-unit (not x)))))

(define (con e1 e2)
  (pv-bind e1
           (lambda (v1)
             (if v1
                 e2
                 (pv-unit #f)))))

(define (dis e1 e2)
  (pv-bind e1
           (lambda (v1)
             (if v1
                 (pv-unit #t)
                 e2))))

(define (if_ et e1 e2)
  (pv-bind et (lambda (t)
                (if t
                    (e1)
                    (e2)))))

(define (lam e)
  (pv-unit (lambda (x)
             (e (pv-unit x)))))

(define (app e1 e2)
  (pv-bind e1 (lambda (f)
                (pv-bind e2 f))))


|#

(define (b x) x)

(define (dist ch)
  (shift k
         (map (lambda (p)
                (cons (car p)
                      (defer (k (cdr p)))))
              ch)))

(define (neg e) (not e))

(define (con e1 e2) (and e1 e2))

(define (dis e1 e2) (or e1 e2))

(define (if_ et e1 e2)
  (if et
      (e1)
      (e2)))


(define (lam e) e)

(define (app e1 e2)
  (e1 e2))

(define (reify0 m)
  (reset (pv-unit (m))))

(define-syntax if-
  (syntax-rules ()
    ((if- ?t ?c ?a)
     (if_ ?t (lambda () ?c) (lambda () ?a)))))

(define (flip p)
  (dist (list (cons p #t)
              (cons (- 1.0 p) #f))))

(define fail (dist '()))

(define (let_ e f)
  (app (lam f) e))

(define-syntax let-
  (syntax-rules ()
    ((let- ?v ?e ?b)
     (let_ ?e (lambda (?v) ?b)))))

(define (grass-is-wet? rain sprinkler)
  (dis (con (flip 0.9) rain)
       (dis (con (flip 0.8) sprinkler)
	    (flip 0.1))))

(define (grass-model)
  (let_ (flip 0.3) 
        (lambda (rain)
          (let_ (flip 0.5) 
                (lambda (sprinkler)
                  (let_ (grass-is-wet? rain sprinkler)
                        (lambda (giw?)
                          (if_ giw?
                               (lambda () rain)
                               (lambda () fail)))))))))

(define (grass-model-1)
  (let- rain (flip 0.3) 
    (let- sprinkler (flip 0.5) 
      (let- giw? (grass-is-wet? rain sprinkler)
        (if- giw?
             rain
             fail)))))

(define (grass-is-wet?/ rain sprinkler)
  (or (and (flip 0.9) rain)
      (or (and (flip 0.8) sprinkler)
	  (flip 0.1))))

(define (grass-model-2)
  (let ((rain (flip 0.3))
        (sprinkler (flip 0.5)))
    (if (grass-is-wet? rain sprinkler)
	rain
	fail)))

(explore #f (reify0 grass-model-2))
