 &gt; &gt; ;; Number -&gt; (listof Image)<br>
 &gt; &gt; ;; produce a list of images suitable for winding<br>
 &gt; &gt; ;; !!!<br>
 &gt; &gt; (check-expect (sieve 1) (list B))<br>
 &gt; &gt; (check-expect (sieve 9) (list B R R W R W R W W))<br>
 &gt; &gt; (check-expect (sieve 25) (list B R R W R W R W B W R W R W R B R =
 W R W R W<br>
 &gt; &gt; R W B))<br>
 &gt; &gt;<br>
 &gt; &gt; (define (sieve n) empty) ;stub<br>
