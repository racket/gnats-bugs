 > Robby
 > 
 > (require (lib "mred.ss" "mred")
 >           (lib "class.ss"))
 > 
 >  (define first? #t)
 > 
 >  (define exn #f)
 > 
 >  (define (m)
 >    (hash-table-map
 >     (ormap (Î» (x) (and (hash-table? x) x))
 >            (continuation-mark-set->list
 >             (exn-continuation-marks exn)
 >             'drscheme-debug-continuation-mark-key))
 >     list))
 > 
 > 
 >  (define weird-canvas%
 >    (class canvas%
 >      (define/override (on-event event)
 >        (super on-event event)
 >        (when first?
 >          (set! first? #f)
 >          (with-handlers ((exn? (lambda (x) (set! exn x) (raise exn))))
 >            (whoops-not-bound))))
 >      (super-new)))
 > 
 >  (define f (new frame% (label "bug") (width 400) (height 400)))
