    (define (master-tick bif)
      (cond
        [(= (tick-food-t (biff-tick bif)) 1) bif]
        [(false? (string=?(person-head (pah-person (biff-pa bif))) "none")) bif]
        [else (tick-handler bif)]))
    
    (define (tick-handler bif)
      (cond
