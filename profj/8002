 >>>     this.left = left;
 >>>     this.info = info;
 >>>     this.right = right;
 >>>   }
 >>>
 >>>   // think of this as the marshalling part for now
 >>>   public String toString() {
 >>>     return "new Node(" + left.toString() + "," + right.toString()  
 >>> + ")";
 >>>   }
 >>> }
 >>>
 >>> // a binary search tree
 >>> class  NodeSearch extends Node {
 >>>
 >>>   NodeSearch(NodeSearch lft, int info, NodeSearch rgt) {
 >>>    super(lft,info,rgt);
 >>>
 >>>    if ((lft != null) && (lft.info < info))
 >>>      okay();
 >>>    else if (lft == null)
 >>>      okay();
 >>>    else
 >>>      Util.error("not a BST");
 >>>
 >>>    if ((rgt !=null) && (info < rgt.info))
 >>>      okay();
 >>>    else if (rgt == null)
 >>>      okay();
 >>>    else
 >>>      Util.error("not a BST");
 >>>   }
 >>>
 >>>   private void okay(){ return ; }
 >>>
 >>>   // does _this_ BST contain a node whose info is equal to key?
 >>>   public boolean findBST(int key) {
 >>>    if (this.info == key)
 >>>     return true;
 >>>    else
 >>>     return ((findAux(this.left,key)) || findAux(this.right,key));
 >>>   }
 >>>
 >>>   // auxiliary for findBST (abstracting out silly stuff)
 >>>   private boolean findAux(Node ptr,int key) {
 >>>    if (ptr == null)
 >>>      return false;
 >>>    else
 >>>      return ((NodeSearch)ptr).findBST(key);
 >>>   }
 >>> }
 >>>
 >>> // examples
 >>> class Examples {
 >>>   NodeSearch n1 = new NodeSearch(null,1,null);
 >>>   NodeSearch n3 = new NodeSearch(null,3,null);
 >>>   NodeSearch n2 = new NodeSearch(n1,2,n3);
 >>>
 >>>   public boolean test1() {
 >>>     return n2.findBST(2);
 >>>   }
 >>>   public boolean test2() {
 >>>     return !n3.findBST(4);
 >>>   }
 >>> }
 >>>
 >>> RUN produces
 >>>
 >>> Class or interface .#bst is not known. in: .#bst
 >>>
